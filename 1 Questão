#include <stdio.h>
int main()
{
 int idade;
 float altura;
 char nome[50];
 
 printf("digite sua idade: ");
 scanf("%d", &idade);
 
 
 printf("digite sua altura: ");
 scanf("%f", &altura);
 
 printf("digite seu nome: ");
 scanf("%s", nome);
 
 printf("Ola %s! VocÃª tem %d e mede %.2f", nome, idade, altura);
}
