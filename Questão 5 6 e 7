// Online C compiler to run C program online
#include <stdio.h>

int main() {

int nota;

printf("Digite sua nota: ");
scanf("%d", &nota);

if (nota <0){
printf("nota invalida");
} else if (nota >10){
printf("nota invalida");
} else if (nota == 10) {
printf("Aprovado com excelência!");
} else if (nota >= 6) {
printf("Aprovado.\n");
} else if (nota >= 5) {
printf("Recuperação.\n");
} else {
printf("Reprovado.\n");
}
    return 0;
}
